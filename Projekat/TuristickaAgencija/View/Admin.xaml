        </ListBox>
        <Button x:Name="button" Content="Briši" HorizontalAlignment="Left" Margin="570,137,0,0" VerticalAlignment="Top" Width="60"/>
        <Button x:Name="button1" Content="Dodaj" HorizontalAlignment="Left" Margin="570,224,0,0" VerticalAlignment="Top"/>
        <AppBarButton x:Name="appBarButton" HorizontalAlignment="Left" Icon="Back" Label="Nazad" Margin="940,625,0,0" VerticalAlignment="Top" Width="119"/>

